extends layout

block layout-content
	div.View.UserView
		h1.Banner Hola, #{userProfile.nickname}!
		div.Lista
			h2 'Lista de Clientes'
			table(border='1').listing-table.table-dark.table-striped
				tr
					th Nombres
					th Apellidos
					th Celular
					th Correo
					th Acciones
				for cliente in clienteList
					tr
						td= cliente.nombres
						td= cliente.apellidos
						td= cliente.celular
						td= cliente.correo
						td
							a(href='/cliente/'+cliente._id) Editar
		div.NavButtons
			a(href="/logout")
				div.NavButton Log out
